{
  "name": "omnizap-site",
  "version": "1.0.1",
  "description": "Site oficial do projeto OmniZap com informações e estatísticas do GitHub",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build:css": "tailwindcss build public/styles.css -o public/dist/styles.css",
    "watch:css": "tailwindcss build public/styles.css -o public/dist/styles.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kaikygr/omnizap.git"
  },
  "keywords": [
    "omnizap",
    "whatsapp",
    "bot",
    "dashboard",
    "estatísticas",
    "github"
  ],
  "author": "Kaikygr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kaikygr/omnizap/issues"
  },
  "homepage": "https://omnizap.shop",
  "dependencies": {
    "cheerio": "^1.0.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-useragent": "^1.0.15",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
